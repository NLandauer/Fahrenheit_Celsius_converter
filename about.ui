<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="ttk.Frame" id="top_frame">
    <property name="height">200</property>
    <property name="padding">10</property>
    <property name="width">200</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Label" id="title_label">
        <property name="font">{Calibri} 20 {bold}</property>
        <property name="justify">center</property>
        <property name="padding">10</property>
        <property name="text" translatable="yes">Temperature Converter App</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label1">
        <property name="font">{Calibri} 12 {}</property>
        <property name="text" translatable="yes">Featuring an About tab,</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label2">
        <property name="font">{Calibri} 12 {}</property>
        <property name="text" translatable="yes">a Celsius to Fahrenheit calculator,</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label3">
        <property name="font">{Calibri} 12 {}</property>
        <property name="text" translatable="yes">and a Fahrenheit to Celsius calculator</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label4">
        <property name="font">{Calibri} 12 {}</property>
        <property name="text" translatable="yes">By Casey Hill and Noelle Landauer</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="pady">10</property>
          <property name="row">6</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
